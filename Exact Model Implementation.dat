//Open the parameter & output sheet
 SheetConnection sheet("ExactModelData.xlsx");

// Indexing arrays
 locations from SheetRead(sheet, "Locations");
 stores from SheetRead(sheet, "Stores");
 horses from SheetRead(sheet, "Horses");
 trailers from SheetRead(sheet, "Trailers");
 
// 1D parameters
 distanceCost from SheetRead(sheet, "DistanceCost");
 timeCost from SheetRead(sheet, "TimeCost");
